{
  "name": "abodely_reviews",
  "version": "1.0.0",
  "description": "Generates, stores, and displays reviews for listings on Abodely",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --verbose",
    "build:watch": "webpack --watch -d",
    "build": "webpack",
    "start": "nodemon ./server/index.js",
    "seed": "node ./server/seed_reviews.js",
    "execute": "webpack ; node ./server/seed_reviews.js ; node ./server/index.js"
  },
  "engines": {
    "node": "^11.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abode-ly/reviews_module.git"
  },
  "author": "Marissa Shieh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abode-ly/reviews_module/issues"
  },
  "homepage": "https://github.com/abode-ly/reviews_module#readme",
  "jest": {
    "setupTestFrameworkScriptFile": "jest-enzyme",
    "testEnvironment": "enzyme",
    "setupFiles": [
      "<rootDir>/enzyme.config.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "css-loader": "^2.1.1",
    "dompurify": "^1.0.10",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "morgan": "^1.9.0",
    "npm": "^6.9.1-next.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sinon": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16.2": "^1.5.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "nodemon": "^1.18.10",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  }
}
